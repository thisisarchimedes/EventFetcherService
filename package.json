{
  "name": "event-fetcher-backend",
  "version": "1.0.0",
  "description": "Backend for Archimedes Leverage",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node --loader ts-node/esm index.ts",
    "lint": "npx eslint --ext .ts,.tsx -c .eslintrc.cjs .",
    "test": "npx nyc --reporter=lcov --reporter=text hardhat test",
    "test:unit": "npx nyc --reporter=lcov --reporter=text hardhat test test/unit/*.test.ts",
    "test:acceptance": "npx nyc --reporter=lcov --reporter=text hardhat test test/acceptance/*.test.ts",
    "build": "rm -rf dist && npx tsc && mv dist/src/* dist/ && rm -r dist/src"
  },
  "dependencies": {
    "@aws-sdk/client-appconfig": "^3.515.0",
    "@thisisarchimedes/backend-sdk": "1.0.59",
    "aws-sdk": "^2.1561.0",
    "aws-sdk-client-mock": "^3.0.1",
    "aws-sdk-mock": "^5.9.0",
    "dotenv": "^16.4.4",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^5.7.2",
    "fast-diff": "^1.3.0",
    "fp-ts": "^2.16.2",
    "lodash": "^4.17.21",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.5.1",
    "nyc": "^15.1.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@stryker-mutator/core": "^8.0.0",
    "@stryker-mutator/mocha-runner": "^8.0.0",
    "@types/chai-as-promised": "^7.1.8",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.4",
    "@types/node": "^20.8.9",
    "@types/sinon": "^17.0.1",
    "@types/sinon-chai": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "axios": "^1.6.2",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-mocha": "^10.2.0",
    "ethereum-waffle": "^4.0.10",
    "hardhat": "^2.19.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.2",
    "typescript": "^5.2.2"
  }
